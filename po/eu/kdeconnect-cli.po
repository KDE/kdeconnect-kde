msgstr "Jaso gailuari buruzko zifratze informazioa"

#: kdeconnect-cli.cpp:100
#, kde-format
msgid "List remote commands and their ids"
msgstr "Lerrendatu urruneko komandoak eta haien IDak"

#: kdeconnect-cli.cpp:101
#, kde-format
msgid "Execute remote command by id"
msgstr "Exekutatu urreneko komandoa IDaren bidez"

#: kdeconnect-cli.cpp:102
#, kde-format
msgid "Send keys to device"
msgstr "Bidali teklak gailura"

#: kdeconnect-cli.cpp:103
