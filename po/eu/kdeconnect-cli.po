#: kdeconnect-cli.cpp:32
#, kde-format
msgid "KDE Connect CLI tool"
msgstr "KDE Connect CLI tresna"

#: kdeconnect-cli.cpp:34
#, kde-format
msgid "© 2015–2025, KDE Connect Team"
msgstr "© 2015-2025, KDE Connect taldea"

#: kdeconnect-cli.cpp:35
#, kde-format
msgid "Aleix Pol Gonzalez"
msgstr "Aleix Pol Gonzalez"

#: kdeconnect-cli.cpp:36
#, kde-format
msgid "Albert Vaca Cintora"
msgstr "Albert Vaca Cintora"

#: kdeconnect-cli.cpp:42
#, kde-format
msgid "List all devices"
msgstr "Zerrendatu gailu guztiak"

#: kdeconnect-cli.cpp:44
#, kde-format
msgid "List available (paired and reachable) devices"
msgstr "Zerrendatu gailu erabilgarriak (parekatuak eta irisgarriak)"

#: kdeconnect-cli.cpp:45
#, kde-format
msgid "List all backends and status"
msgstr "Zerrendatu bizkarraldeko denak eta egoera denak"

#: kdeconnect-cli.cpp:46
#, kde-format
msgid "Enable the specified backend"
msgstr "Gaitu adierazitako bizkarraldekoa"

#: kdeconnect-cli.cpp:46 kdeconnect-cli.cpp:47
#, kde-format
msgid "backend"
msgstr "bizkarraldekoa"

#: kdeconnect-cli.cpp:47
#, kde-format
msgid "Disable the specified backend"
msgstr "Ezgaitu adierazitako bizkarraldekoa"

#: kdeconnect-cli.cpp:49
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id, to ease "
"scripting"
msgstr ""
"Gidoiak egitea errazteko, --list-devices edo --list-available aukerek "
"gailuen IDak bakarrik inprimatu ditzaten egin"

#: kdeconnect-cli.cpp:51
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices name, to ease "
"scripting"
msgstr ""
"Gidoiak egitea errazteko, --list-devices edo --list-available aukerek "
"gailuen izenak bakarrik inprimatu ditzaten egin"

#: kdeconnect-cli.cpp:53
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id and name, "
"to ease scripting"
msgstr ""
"Gidoiak egitea errazteko, --list-devices edo --list-available aukerek "
"gailuen ID eta izenak bakarrik inprimatu ditzaten egin"

#: kdeconnect-cli.cpp:54
#, kde-format
msgid "Search for devices in the network and re-establish connections"
msgstr "Bilatu gailuak sarean eta konexioak birsortu"

#: kdeconnect-cli.cpp:55
#, kde-format
msgid "Request pairing to a said device"
msgstr "Eskatu adierazitako gailuarekin parekatzea"

#: kdeconnect-cli.cpp:56
#, kde-format
msgid "Find the said device by ringing it."
msgstr "Aurkitu adierazitako gailua bere dei-doinua joaz."

#: kdeconnect-cli.cpp:57
#, kde-format
msgid "Stop pairing to a said device"
msgstr "Gelditu adierazitako gailuarekiko parekatzea"

#: kdeconnect-cli.cpp:58
#, kde-format
msgid "Sends a ping to said device"
msgstr "Adierazitako gailura ping bat bidaltzen du"

#: kdeconnect-cli.cpp:59
#, kde-format
msgid "Same as ping but you can set the message to display"
msgstr "Ping-aren berdina, baina azaldu beharreko mezua ezar dezakezu"

#: kdeconnect-cli.cpp:59 kdeconnect-cli.cpp:66
#, kde-format
msgid "message"
msgstr "mezua"

#: kdeconnect-cli.cpp:60
#, kde-format
msgid "Sends the current clipboard to said device"
msgstr "Uneko arbela adierazitako gailura bidaltzen du"

#: kdeconnect-cli.cpp:61
#, kde-format
msgid "Share a file/URL to a said device"
msgstr "Partekatu fitxategi/URL bat adierazitako gailu batekin"

#: kdeconnect-cli.cpp:62
#, kde-format
msgid "Share text to a said device"
msgstr "Partekatu testua adierazitako gailuarekin"

#: kdeconnect-cli.cpp:63
#, kde-format
msgid "Display the notifications on a said device"
msgstr "Azaldu jakinarazpenak adierazitako gailuan"

#: kdeconnect-cli.cpp:64
#, kde-format
msgid "Lock the specified device"
msgstr "Giltzatu zehaztutako gailua"

#: kdeconnect-cli.cpp:65
#, kde-format
msgid "Unlock the specified device"
msgstr "Giltzapetik askatu adierazitako gailua"

#: kdeconnect-cli.cpp:66
#, kde-format
msgid "Sends an SMS. Requires destination"
msgstr "SMS bat bidaltzen du. Hartzailea behar du"

#: kdeconnect-cli.cpp:67
#, kde-format
msgid "Phone number to send the message"
msgstr "Mezua bidaltzeko telefono zenbakia"

#: kdeconnect-cli.cpp:67
#, kde-format
msgid "phone number"
msgstr "telefono zenbakia"

#: kdeconnect-cli.cpp:69
#, kde-format
msgid ""
"File URLs to send attachments with the message (can be passed multiple times)"
msgstr ""
"Mezuarekin batera bidaliko diren fitxategien URLak (hainbat aldiz pasa "
"daiteke)"

#: kdeconnect-cli.cpp:70
#, kde-format
msgid "file URLs"
msgstr "fitxategien URLak"

#: kdeconnect-cli.cpp:71
#, kde-format
msgid "Device ID"
msgstr "Gailuaren ID"

#: kdeconnect-cli.cpp:72
#, kde-format
msgid "Device Name"
msgstr "Gailuaren izena"

#: kdeconnect-cli.cpp:73
#, kde-format
msgid "Get encryption info about said device"
msgstr "Jaso adierazitako gailuari buruzko zifratze informazioa"

#: kdeconnect-cli.cpp:74
#, kde-format
msgid "Lists remote commands and their ids"
msgstr "Urruneko komandoak eta haien IDak zerrendatzen ditu"

#: kdeconnect-cli.cpp:75
#, kde-format
msgid "Executes a remote command by id"
msgstr "ID bidez urruneko agindu bat exekutatzen du"

#: kdeconnect-cli.cpp:77
#, kde-format
msgid "Sends keys to a said device"
msgstr "Teklak adierazitako gailura bidaltzen ditu"

#: kdeconnect-cli.cpp:78
#, kde-format
msgid "Display this device's id and exit"
msgstr "Gailu honen ID azaldu eta irten egiten da"

#: kdeconnect-cli.cpp:124
#, kde-format
msgid "(paired and reachable)"
msgstr "(parekatuta eta eskuragarri)"

#: kdeconnect-cli.cpp:126
#, kde-format
msgid "(reachable)"
msgstr "(eskuragarri)"

#: kdeconnect-cli.cpp:128 kdeconnect-cli.cpp:152
#, kde-format
msgid "(paired)"
msgstr "(parekatuta)"

#: kdeconnect-cli.cpp:134
#, kde-format
msgid "1 device found"
msgid_plural "%1 devices found"
msgstr[0] "gailu 1 aurkitu da"
msgstr[1] "%1 gailu aurkitu dira"

#: kdeconnect-cli.cpp:136
#, kde-format
msgid "No devices found"
msgstr "Ez da gailurik aurkitu"

#: kdeconnect-cli.cpp:154
#, kde-format
msgid "(unpaired)"
msgstr "(parekatu gabe)"

#: kdeconnect-cli.cpp:197
#, kde-format
msgid ""
"No device specified: Use -d <Device ID> or -n <Device Name> to specify a "
"device. \n"
"Device ID's and names may be found using \"kdeconnect-cli -l\" \n"
"View complete help with --help option"
msgstr ""
"Ez da gailurik zehaztu: Erabili -d <Gailu ID> edo -n <Gailuaren izena> gailu "
"bat zehazteko. \n"
"Gailu IDak eta izenak \"kdeconnect-cli -l\" erabiliz aurki daitezke.\n"
"Ikusi laguntza osoa --help aukera erabiliz."

#: kdeconnect-cli.cpp:232
#, kde-format
msgid "Shared %1"
msgstr "%1 partekatuta"

#: kdeconnect-cli.cpp:241
#, kde-format
msgid "Shared text: %1"
msgstr "Partekatutako testua: %1"

#: kdeconnect-cli.cpp:248
#, kde-format
msgctxt "device has requested to lock peer device"
msgid "Requested to lock %1."
msgstr "%1 giltzatzea eskatu da."

#: kdeconnect-cli.cpp:250
#, kde-format
msgctxt "device has requested to unlock peer device"
msgid "Requested to unlock %1."
msgstr "%1 giltzapetik askatzea eskatu da."

#: kdeconnect-cli.cpp:257
#, kde-format
msgid "waiting for device…"
msgstr "gailuaren zain..."

#: kdeconnect-cli.cpp:270
#, kde-format
msgid "Device not found"
msgstr "Gailua ez da aurkitu"

#: kdeconnect-cli.cpp:272
#, kde-format
msgid "Already paired"
msgstr "Dagoeneko parekatuta"

#: kdeconnect-cli.cpp:274
#, kde-format
msgid "Pair requested"
msgstr "Parekatzea eskatu da"

#: kdeconnect-cli.cpp:280
#, kde-format
msgid "Already not paired"
msgstr "Dagoeneko parekatua gabe"

#: kdeconnect-cli.cpp:282
#, kde-format
msgid "Unpaired"
msgstr "Desparekatua"

#: kdeconnect-cli.cpp:328
#, kde-format
msgid ""
"error: should specify the SMS's recipient by passing --destination <phone "
"number>"
msgstr ""
"errorea: SMSaren hartzailea  --destination pasatuz zehaztu beharko "
"litzateke<phone number>"

