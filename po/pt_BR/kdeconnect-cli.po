#: kdeconnect-cli.cpp:38
#, kde-format
msgid ""
"No device specified: Use -d <Device ID> or -n <Device Name> to specify a "
"device. \n"
"Device ID's and names may be found using \"kdeconnect-cli -l\" \n"
"View complete help with --help option"
msgstr ""
"Nenhum dispositivo indicado: Use -d <Device ID> ou -n <Device Name> para "
"indicar um dispositivo. \n"
"Os IDs e nomes dos dispositivos podem ser obtidos com \"kdeconnect-cli -l"
"\" \n"
"Consulte a ajuda completa com a opção --help"

#: kdeconnect-cli.cpp:58
#, kde-format
msgid "KDE Connect CLI tool"
msgstr "Ferramenta da linha de comando do KDE-Connect"

#: kdeconnect-cli.cpp:60
#, kde-format
msgid "© 2015–2025, KDE Connect Team"
msgstr "© 2015–2025 Equipe do KDE Connect"

#: kdeconnect-cli.cpp:61
#, kde-format
msgid "Aleix Pol Gonzalez"
msgstr "Aleix Pol Gonzalez"

#: kdeconnect-cli.cpp:62
#, kde-format
msgid "Albert Vaca Cintora"
msgstr "Albert Vaca Cintora"

#: kdeconnect-cli.cpp:68
#, kde-format
msgid "List all devices"
msgstr "Listar todos os dispositivos"

#: kdeconnect-cli.cpp:70
#, kde-format
msgid "List available (paired and reachable) devices"
msgstr "Lista dos dispositivos disponíveis (emparelhados e acessíveis)"

#: kdeconnect-cli.cpp:71
#, kde-format
msgid "List all backends and status"
msgstr "Listar todas as infraestruturas e status"

#: kdeconnect-cli.cpp:72
#, kde-format
msgid "Enable the specified backend"
msgstr "Habilitar a infraestrutura indicada"

#: kdeconnect-cli.cpp:72 kdeconnect-cli.cpp:73
#, kde-format
msgid "backend"
msgstr "infraestrutura"

#: kdeconnect-cli.cpp:73
#, kde-format
msgid "Disable the specified backend"
msgstr "Desabilitar a infraestrutura indicada"

#: kdeconnect-cli.cpp:75
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id, to ease "
"scripting"
msgstr ""
"Fazer o --list-devices ou o --list-available imprimir apenas o ID dos "
"dispositivos, para facilitar a criação de scripts"

#: kdeconnect-cli.cpp:77
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices name, to ease "
"scripting"
msgstr ""
"Fazer o --list-devices ou o --list-available imprimir apenas o nome dos "
"dispositivos, para facilitar a criação de scripts"

#: kdeconnect-cli.cpp:79
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id and name, "
"to ease scripting"
msgstr ""
"Fazer o --list-devices ou o --list-available imprimir apenas o nome e o ID "
"dos dispositivos, para facilitar a criação de scripts"

#: kdeconnect-cli.cpp:80
#, kde-format
msgid "Search for devices in the network and re-establish connections"
msgstr "Procurar dispositivos na rede e restabelecer conexões"

#: kdeconnect-cli.cpp:81
#, kde-format
msgid "Request pairing with device"
msgstr "Solicitar o emparelhamento com dispositivo"

#: kdeconnect-cli.cpp:82
#, kde-format
msgid "Find device by ringing it"
msgstr "Encontrar o dispositivo fazendo com que ele toque"

#: kdeconnect-cli.cpp:83
#, kde-format
msgid "Unpair from device"
msgstr "Desemparelhar do dispositivo"

#: kdeconnect-cli.cpp:84
#, kde-format
msgid "Send ping to device"
msgstr "Enviar ping para dispositivo"

#: kdeconnect-cli.cpp:85
#, kde-format
msgid "Same as ping but you can set the message to display"
msgstr "Igual ao ping mas você pode definir a mensagem apresentada"

#: kdeconnect-cli.cpp:85 kdeconnect-cli.cpp:92
#, kde-format
msgid "message"
msgstr "mensagem"

#: kdeconnect-cli.cpp:86
#, kde-format
msgid "Send clipboard to device"
msgstr "Enviar a área de transferência para dispositivo"

#: kdeconnect-cli.cpp:87
#, kde-format
msgid "Share file/URL to device"
msgstr "Compartilhar arquivo/URL com dispositivo"

#: kdeconnect-cli.cpp:88
#, kde-format
msgid "Share text to device"
msgstr "Compartilhar texto com dispositivo"

#: kdeconnect-cli.cpp:89
#, kde-format
msgid "Display notifications on device"
msgstr "Mostrar notificações no dispositivo"

#: kdeconnect-cli.cpp:90
#, kde-format
msgid "Lock the specified device"
msgstr "Bloquear o dispositivo indicado"

#: kdeconnect-cli.cpp:91
#, kde-format
msgid "Unlock the specified device"
msgstr "Desbloquear o dispositivo indicado"

#: kdeconnect-cli.cpp:92
#, kde-format
msgid "Sends an SMS. Requires destination"
msgstr "Enviar um SMS. Requer destinatário"

#: kdeconnect-cli.cpp:93
#, kde-format
msgid "Phone number to send the message"
msgstr "Número de telefone para enviar a mensagem"

#: kdeconnect-cli.cpp:93
#, kde-format
msgid "phone number"
msgstr "número de telefone"

#: kdeconnect-cli.cpp:95
#, kde-format
msgid ""
"File URLs to send attachments with the message (can be passed multiple times)"
msgstr ""
"URLs de arquivos para enviar anexos com a mensagem (pode ser usado múltiplas "
"vezes)"

#: kdeconnect-cli.cpp:96
#, kde-format
msgid "file URLs"
msgstr "URLs de arquivo"

#: kdeconnect-cli.cpp:97
#, kde-format
msgid "Device ID"
msgstr "ID do dispositivo"

#: kdeconnect-cli.cpp:98
#, kde-format
msgid "Device Name"
msgstr "Nome do dispositivo"

#: kdeconnect-cli.cpp:99
#, kde-format
msgid "Get encryption info about device"
msgstr "Obter informações de criptografia sobre dispositivo"

#: kdeconnect-cli.cpp:100
#, kde-format
msgid "List remote commands and their ids"
msgstr "Listar comandos remotos e seus IDs"

#: kdeconnect-cli.cpp:101
#, kde-format
msgid "Execute remote command by id"
msgstr "Executar comando remoto pelo seu ID"

#: kdeconnect-cli.cpp:102
#, kde-format
msgid "Send keys to device"
msgstr "Envia teclas para dispositivo"

#: kdeconnect-cli.cpp:103
#, kde-format
msgid "Display this device's id and exit"
msgstr "Mostrar o ID deste dispositivo e sair"

#: kdeconnect-cli.cpp:104
#, kde-format
msgid "Mount filesystem of a said device"
msgstr "Montar sistema de arquivos de um determinado dispositivo"

#: kdeconnect-cli.cpp:105
#, kde-format
msgid "Print mount point for the filesystem of a said device"
msgstr ""
"Imprimir ponto de montagem para o sistema de arquivos de um determinado "
"dispositivo"

