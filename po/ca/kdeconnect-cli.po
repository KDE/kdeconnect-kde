#: kdeconnect-cli.cpp:42
#, kde-format
msgid "List all devices"
msgstr "Llista tots els dispositius"

#: kdeconnect-cli.cpp:44
#, kde-format
msgid "List available (paired and reachable) devices"
msgstr "Llista tots els dispositius disponibles (aparellats i accessibles)"

#: kdeconnect-cli.cpp:45
#, kde-format
msgid "List all backends and status"
msgstr "Llista tots els dorsals i els estats"

#: kdeconnect-cli.cpp:46
#, kde-format
msgid "Enable the specified backend"
msgstr "Activa el dorsal especificat"

#: kdeconnect-cli.cpp:46 kdeconnect-cli.cpp:47
#, kde-format
msgid "backend"
msgstr "dorsal"

#: kdeconnect-cli.cpp:47
#, kde-format
msgid "Disable the specified backend"
msgstr "Desactiva el dorsal especificat"

#: kdeconnect-cli.cpp:49
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id, to ease "
"scripting"
msgstr ""
"Fa que --list-devices o --list-available imprimeixi només els ID dels "
"dispositius, per a facilitar la creació de scripts"

#: kdeconnect-cli.cpp:51
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices name, to ease "
"scripting"
msgstr ""
"Fa que --list-devices o --list-available imprimeixi només els noms dels "
"dispositius, per a facilitar la creació de scripts"

#: kdeconnect-cli.cpp:53
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id and name, "
"to ease scripting"
msgstr ""
"Fa que --list-devices o --list-available imprimeixi només els ID i els noms "
"dels dispositius, per a facilitar la creació de scripts"

#: kdeconnect-cli.cpp:54
#, kde-format
msgid "Search for devices in the network and re-establish connections"
msgstr "Cerca dispositius a la xarxa i torna a establir les connexions"

#: kdeconnect-cli.cpp:55
#, kde-format
msgid "Request pairing to a said device"
msgstr "Demana aparellar amb un dispositiu indicat"

#: kdeconnect-cli.cpp:56
#, kde-format
msgid "Find the said device by ringing it."
msgstr "Troba el dispositiu indicat trucant-li."

#: kdeconnect-cli.cpp:57
#, kde-format
msgid "Stop pairing to a said device"
msgstr "Atura l'aparellament amb un dispositiu indicat"

#: kdeconnect-cli.cpp:58
#, kde-format
msgid "Sends a ping to said device"
msgstr "Envia un ping a un dispositiu indicat"

#: kdeconnect-cli.cpp:59
#, kde-format
msgid "Same as ping but you can set the message to display"
msgstr "Igual que el ping, però es pot definir el missatge que es mostrarà"

#: kdeconnect-cli.cpp:59 kdeconnect-cli.cpp:66
#, kde-format
msgid "message"
msgstr "missatge"

#: kdeconnect-cli.cpp:60
#, kde-format
msgid "Sends the current clipboard to said device"
msgstr "Envia el porta-retalls actual a un dispositiu indicat"

#: kdeconnect-cli.cpp:61
#, kde-format
msgid "Share a file/URL to a said device"
msgstr "Comparteix un fitxer/URL amb un dispositiu indicat"

#: kdeconnect-cli.cpp:62
#, kde-format
msgid "Share text to a said device"
msgstr "Comparteix text a un dispositiu indicat"

#: kdeconnect-cli.cpp:63
#, kde-format
msgid "Display the notifications on a said device"
msgstr "Mostra les notificacions a un dispositiu indicat"

#: kdeconnect-cli.cpp:64
#, kde-format
msgid "Lock the specified device"
msgstr "Bloqueja el dispositiu especificat"

#: kdeconnect-cli.cpp:65
#, kde-format
msgid "Unlock the specified device"
msgstr "Desbloqueja el dispositiu especificat"

#: kdeconnect-cli.cpp:66
#, kde-format
msgid "Sends an SMS. Requires destination"
msgstr "Envia un SMS. Requereix una destinació"

#: kdeconnect-cli.cpp:67
#, kde-format
msgid "Phone number to send the message"
msgstr "Número de telèfon a on enviar el missatge"

#: kdeconnect-cli.cpp:67
#, kde-format
msgid "phone number"
msgstr "número de telèfon"

#: kdeconnect-cli.cpp:69
#, kde-format
msgid ""
"File URLs to send attachments with the message (can be passed multiple times)"
msgstr ""
"URL dels fitxers a enviar adjuntats al missatge (es pot indicar diverses "
"vegades)"

#: kdeconnect-cli.cpp:70
#, kde-format
msgid "file URLs"
msgstr "URL dels fitxers"

#: kdeconnect-cli.cpp:71
#, kde-format
msgid "Device ID"
msgstr "ID del dispositiu"

#: kdeconnect-cli.cpp:72
#, kde-format
msgid "Device Name"
msgstr "Nom del dispositiu"

#: kdeconnect-cli.cpp:73
#, kde-format
msgid "Get encryption info about said device"
msgstr "Obtén la informació d'encriptatge del dispositiu indicat"

#: kdeconnect-cli.cpp:74
#, kde-format
msgid "Lists remote commands and their ids"
msgstr "Llista les ordres remotes i les seves ID"

#: kdeconnect-cli.cpp:75
#, kde-format
msgid "Executes a remote command by id"
msgstr "Executa una ordre remota pel seu ID"

#: kdeconnect-cli.cpp:77
#, kde-format
msgid "Sends keys to a said device"
msgstr "Envia les tecles a un dispositiu indicat"

#: kdeconnect-cli.cpp:78
#, kde-format
msgid "Display this device's id and exit"
msgstr "Mostra l'ID d'aquest dispositiu i surt"

#: kdeconnect-cli.cpp:124
#, kde-format
msgid "(paired and reachable)"
msgstr "(aparellat i accessible)"

#: kdeconnect-cli.cpp:126
#, kde-format
msgid "(reachable)"
msgstr "(accessible)"

#: kdeconnect-cli.cpp:128 kdeconnect-cli.cpp:152
#, kde-format
msgid "(paired)"
msgstr "(aparellat)"

#: kdeconnect-cli.cpp:134
#, kde-format
msgid "1 device found"
msgid_plural "%1 devices found"
msgstr[0] "S'ha trobat 1 dispositiu"
msgstr[1] "S'han trobat %1 dispositius"

#: kdeconnect-cli.cpp:136
#, kde-format
msgid "No devices found"
msgstr "No s'ha trobat cap dispositiu"

#: kdeconnect-cli.cpp:154
#, kde-format
msgid "(unpaired)"
msgstr "(sense aparellar)"

#: kdeconnect-cli.cpp:197
#, kde-format
msgid ""
"No device specified: Use -d <Device ID> or -n <Device Name> to specify a "
"device. \n"
"Device ID's and names may be found using \"kdeconnect-cli -l\" \n"
"View complete help with --help option"
msgstr ""
"No s'ha especificat cap dispositiu: useu -d <ID_de_dispositiu> o -n "
"<Nom_del_dispositiu> per a especificar un dispositiu.\n"
"Els ID i noms dels dispositius es poden trobar usant «kdeconnect-cli -l» \n"
"Vegeu tota l'ajuda amb l'opció --help"

#: kdeconnect-cli.cpp:232
#, kde-format
msgid "Shared %1"
msgstr "Compartit %1"

#: kdeconnect-cli.cpp:241
#, kde-format
msgid "Shared text: %1"
msgstr "Text compartit %1"

#: kdeconnect-cli.cpp:248
#, kde-format
msgctxt "device has requested to lock peer device"
msgid "Requested to lock %1."
msgstr "Sol·licitud de bloqueig %1."

#: kdeconnect-cli.cpp:250
#, kde-format
msgctxt "device has requested to unlock peer device"
msgid "Requested to unlock %1."
msgstr "Sol·licitud de desbloqueig %1."

#: kdeconnect-cli.cpp:257
#, kde-format
msgid "waiting for device…"
msgstr "s'està esperant un dispositiu…"

#: kdeconnect-cli.cpp:270
#, kde-format
msgid "Device not found"
msgstr "No s'ha trobat cap dispositiu"

#: kdeconnect-cli.cpp:272
#, kde-format
msgid "Already paired"
msgstr "Ja està aparellat"

#: kdeconnect-cli.cpp:274
#, kde-format
msgid "Pair requested"
msgstr "S'ha demanat aparellar"

#: kdeconnect-cli.cpp:280
#, kde-format
msgid "Already not paired"
msgstr "Ja no està aparellat"

#: kdeconnect-cli.cpp:282
#, kde-format
msgid "Unpaired"
msgstr "Sense aparellar"

#: kdeconnect-cli.cpp:328
#, kde-format
msgid ""
"error: should specify the SMS's recipient by passing --destination <phone "
"number>"
msgstr ""
"Error: cal especificar el destinatari de l'SMS informant --destination "
"<número de telèfon>"

