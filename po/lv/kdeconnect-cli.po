#: kdeconnect-cli.cpp:38
#, kde-format
msgid ""
"No device specified: Use -d <Device ID> or -n <Device Name> to specify a "
"device. \n"
"Device ID's and names may be found using \"kdeconnect-cli -l\" \n"
"View complete help with --help option"
msgstr ""
"Ierīce nav precizēta: izmantojiet -d <Device ID> vai -n <Device Name>, lai "
"precizētu ierīci. \n"
"Ierīces ID un nosaukumi ir atrodami, izmantojot arī „kdeconnect-cli -l“ \n"
"Pilnīgāku palīdzību meklējiet ar „--help“ parametru"

#: kdeconnect-cli.cpp:58
#, kde-format
msgid "KDE Connect CLI tool"
msgstr "„KDE Connect“ CLI rīks"

#: kdeconnect-cli.cpp:60
#, kde-format
msgid "© 2015–2025, KDE Connect Team"
msgstr "© 2015—2025, „KDE Connect“ komanda"

#: kdeconnect-cli.cpp:61
#, kde-format
msgid "Aleix Pol Gonzalez"
msgstr "Aleix Pol Gonzalez"

#: kdeconnect-cli.cpp:62
#, kde-format
msgid "Albert Vaca Cintora"
msgstr "Albert Vaca Cintora"

#: kdeconnect-cli.cpp:68
#, kde-format
msgid "List all devices"
msgstr "Uzskaitīt visas ierīces"

#: kdeconnect-cli.cpp:70
#, kde-format
msgid "List available (paired and reachable) devices"
msgstr "Uzskaitīt pieejamās (sapārotas un sasniedzamās) ierīces"

#: kdeconnect-cli.cpp:71
#, kde-format
msgid "List all backends and status"
msgstr "Uzskaitīt visas aizmugursistēmas un statusu"

#: kdeconnect-cli.cpp:72
#, kde-format
msgid "Enable the specified backend"
msgstr "Ieslēgt attiecīgo aizmugursistēmu"

#: kdeconnect-cli.cpp:72 kdeconnect-cli.cpp:73
#, kde-format
msgid "backend"
msgstr "aizmugursistēma"

#: kdeconnect-cli.cpp:73
#, kde-format
msgid "Disable the specified backend"
msgstr "Izslēgt attiecīgo aizmugursistēmu"

#: kdeconnect-cli.cpp:75
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id, to ease "
"scripting"
msgstr ""
"Likt „--list-devices“ un „--list-available“ izdrukāt tikai ierīču ID, lai "
"atvieglotu skriptēšanu"

#: kdeconnect-cli.cpp:77
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices name, to ease "
"scripting"
msgstr ""
"Likt „--list-devices“ un „--list-available“ izdrukāt tikai ierīču nosaukumu, "
"lai atvieglotu skriptēšanu"

#: kdeconnect-cli.cpp:79
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id and name, "
"to ease scripting"
msgstr ""
"Likt „--list-devices“ un „--list-available“ izdrukāt tikai ierīču ID un "
"nosaukumu, lai atvieglotu skriptēšanu"

#: kdeconnect-cli.cpp:80
#, kde-format
msgid "Search for devices in the network and re-establish connections"
msgstr "Meklēt ierīces tīklā un atjaunot savienojumus"

#: kdeconnect-cli.cpp:81
#, kde-format
msgid "Request pairing with device"
msgstr "Lūgt sapārošanu ar ierīci"

#: kdeconnect-cli.cpp:82
#, kde-format
msgid "Find device by ringing it"
msgstr "Atrast ierīci, liekot tai atskaņot signālu"

#: kdeconnect-cli.cpp:83
#, kde-format
msgid "Unpair from device"
msgstr "Atpārot ierīci"

#: kdeconnect-cli.cpp:84
#, kde-format
msgid "Send ping to device"
msgstr "Nosūta „ping“ signālu uz ierīci"

#: kdeconnect-cli.cpp:85
#, kde-format
msgid "Same as ping but you can set the message to display"
msgstr "Tas pats, kas „ping“, bet varat norādīt parādāmo ziņojumu"

#: kdeconnect-cli.cpp:85 kdeconnect-cli.cpp:92
#, kde-format
msgid "message"
msgstr "ziņojums"

#: kdeconnect-cli.cpp:86
#, kde-format
msgid "Send clipboard to device"
msgstr "Nosūtīt starpliktuves saturu uz ierīci"

#: kdeconnect-cli.cpp:87
#, kde-format
msgid "Share file/URL to device"
msgstr "Dalīties ar datni/URL ar ierīci"

#: kdeconnect-cli.cpp:88
#, kde-format
msgid "Share text to device"
msgstr "Dalīties ar tekstu ar ierīci"

#: kdeconnect-cli.cpp:89
#, kde-format
msgid "Display notifications on device"
msgstr "Parādīt paziņojumu ierīcē"

#: kdeconnect-cli.cpp:90
#, kde-format
msgid "Lock the specified device"
msgstr "Bloķēt attiecīgo ierīci"

#: kdeconnect-cli.cpp:91
#, kde-format
msgid "Unlock the specified device"
msgstr "Atbloķēt attiecīgo ierīci"

#: kdeconnect-cli.cpp:92
#, kde-format
msgid "Sends an SMS. Requires destination"
msgstr "Nosūtīt īsziņu. Nepieciešams galamērķis"

#: kdeconnect-cli.cpp:93
#, kde-format
msgid "Phone number to send the message"
msgstr "Tālruņa numurs, uz kuru nosūtīt ziņojumu"

#: kdeconnect-cli.cpp:93
#, kde-format
msgid "phone number"
msgstr "tālruņa numurs"

#: kdeconnect-cli.cpp:95
#, kde-format
msgid ""
"File URLs to send attachments with the message (can be passed multiple times)"
msgstr ""
"Datņu URL, kur nosūtīt pielikumus ar ziņojumu (iespējams padot vairākas "
"reizes)"

#: kdeconnect-cli.cpp:96
#, kde-format
msgid "file URLs"
msgstr "datņu URL"

#: kdeconnect-cli.cpp:97
#, kde-format
msgid "Device ID"
msgstr "Ierīces ID"

#: kdeconnect-cli.cpp:98
#, kde-format
msgid "Device Name"
msgstr "Ierīces nosaukums"

#: kdeconnect-cli.cpp:99
#, kde-format
msgid "Get encryption info about device"
msgstr "Iegūt šifrēšanas informāciju par ierīci"

#: kdeconnect-cli.cpp:100
#, kde-format
msgid "List remote commands and their ids"
msgstr "Uzskaitīt attālās vadības komandas un to ID"

#: kdeconnect-cli.cpp:101
#, kde-format
msgid "Execute remote command by id"
msgstr "Izpildīt attālas vadības komandu pēc ID"

#: kdeconnect-cli.cpp:102
#, kde-format
msgid "Send keys to device"
msgstr "Nosūtīt taustiņu spiedienus uz ierīci"

#: kdeconnect-cli.cpp:103
#, kde-format
msgid "Display this device's id and exit"
msgstr "Parāda ierīces ID un iziet"

#: kdeconnect-cli.cpp:104
#, kde-format
msgid "Mount filesystem of a said device"
msgstr "Piemontēt minētās ierīces datņu sistēmu"

#: kdeconnect-cli.cpp:105
#, kde-format
msgid "Print mount point for the filesystem of a said device"
msgstr "Drukāt attiecīgās ierīces datņu sistēmas montēšanas vietu"

#: kdeconnect-cli.cpp:151
#, kde-format
msgid "(paired and reachable)"
msgstr "(sapārots un sasniedzams)"

