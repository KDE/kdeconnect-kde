#: kdeconnect-cli.cpp:68
#, kde-format
msgid "List all devices"
msgstr "Списък на всички устройства"

#: kdeconnect-cli.cpp:70
#, kde-format
msgid "List available (paired and reachable) devices"
msgstr "Списък на устройства (сдвоени и достъпни)"

#: kdeconnect-cli.cpp:71
#, kde-format
msgid "List all backends and status"
msgstr "Показване на състояние и на всички налични бекенд програми"

#: kdeconnect-cli.cpp:72
#, kde-format
msgid "Enable the specified backend"
msgstr "Отключване на посочения бекенд"

#: kdeconnect-cli.cpp:72 kdeconnect-cli.cpp:73
#, kde-format
msgid "backend"
msgstr "бекенд"

#: kdeconnect-cli.cpp:73
#, kde-format
msgid "Disable the specified backend"
msgstr "Деактивиране на посочения бекенд"

#: kdeconnect-cli.cpp:75
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id, to ease "
"scripting"
msgstr ""
"Make --list-devices или --list-available показва само id на устройствата за "
"улесняване на скриптовете"

#: kdeconnect-cli.cpp:77
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices name, to ease "
"scripting"
msgstr ""
"Make --list-devices или --list-available показва само имената на "
"устройствата за улесняване на скриптовете"

#: kdeconnect-cli.cpp:79
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id and name, "
"to ease scripting"
msgstr ""
"Make --list-devices или --list-available показва само id и имената на "
"устройствата за улесняване на скриптовете"

#: kdeconnect-cli.cpp:80
#, kde-format
msgid "Search for devices in the network and re-establish connections"
msgstr "Търсене на устройства в мрежата и повторно установяване на връзки"

#: kdeconnect-cli.cpp:81
#, kde-format
msgid "Request pairing with device"
msgstr "Заявка за сдвояване с устройство"

#: kdeconnect-cli.cpp:82
#, kde-format
msgid "Find device by ringing it"
msgstr "Намиране на устройство чрез позвъняване."

#: kdeconnect-cli.cpp:83
#, kde-format
msgid "Unpair from device"
msgstr "Разкачване от устройство"

#: kdeconnect-cli.cpp:84
#, kde-format
msgid "Send ping to device"
msgstr "Изпраща пинг към устройство"

#: kdeconnect-cli.cpp:85
#, kde-format
msgid "Same as ping but you can set the message to display"
msgstr ""
"Същото като сигнализиране, но можете да зададете съобщение за показване"

#: kdeconnect-cli.cpp:85 kdeconnect-cli.cpp:92
#, kde-format
msgid "message"
msgstr "съобщение"

#: kdeconnect-cli.cpp:86
#, kde-format
msgid "Send clipboard to device"
msgstr "Изпраща съдържанието на клипборда към устройство"

#: kdeconnect-cli.cpp:87
#, kde-format
msgid "Share file/URL to device"
msgstr "Споделяне на файл/URL към  устройство"

#: kdeconnect-cli.cpp:88
#, kde-format
msgid "Share text to device"
msgstr "Споделяне на текст към устройство"

#: kdeconnect-cli.cpp:89
#, kde-format
msgid "Display notifications on device"
msgstr "Показване на известията на устройство"

#: kdeconnect-cli.cpp:90
#, kde-format
msgid "Lock the specified device"
msgstr "Заключване на посоченото устройство"

#: kdeconnect-cli.cpp:91
#, kde-format
msgid "Unlock the specified device"
msgstr "Отключване на посоченото устройство"

#: kdeconnect-cli.cpp:92
#, kde-format
msgid "Sends an SMS. Requires destination"
msgstr "Изпраща SMS. Изисква дестинация"

#: kdeconnect-cli.cpp:93
#, kde-format
msgid "Phone number to send the message"
msgstr "Телефонен номер за изпращане на съобщението"

#: kdeconnect-cli.cpp:93
#, kde-format
msgid "phone number"
msgstr "телефонен номер"

#: kdeconnect-cli.cpp:95
#, kde-format
msgid ""
"File URLs to send attachments with the message (can be passed multiple times)"
msgstr ""
"Файлови URL адреси за изпращане на прикачени файлове със съобщението (може "
"да се предаде няколко пъти)"

#: kdeconnect-cli.cpp:96
#, kde-format
msgid "file URLs"
msgstr "файлови URL адреси"

#: kdeconnect-cli.cpp:97
#, kde-format
msgid "Device ID"
msgstr "Идент. № на устройство"

#: kdeconnect-cli.cpp:98
#, kde-format
msgid "Device Name"
msgstr "Име на устройство"

#: kdeconnect-cli.cpp:99
#, kde-format
msgid "Get encryption info about device"
msgstr "Получаване на информация за шифроване за устройство"

#: kdeconnect-cli.cpp:100
#, kde-format
msgid "List remote commands and their ids"
msgstr "Изброяване на отдалечени команди и техните ids"

#: kdeconnect-cli.cpp:101
#, kde-format
msgid "Execute remote command by id"
msgstr "Изпълняване на     отдалечена команда по id"

#: kdeconnect-cli.cpp:102
#, kde-format
msgid "Send keys to device"
msgstr "Изпращане на ключове към устройство"

#: kdeconnect-cli.cpp:103
#, kde-format
msgid "Display this device's id and exit"
msgstr "Показване на идентификацията и изхода на това устройство"

#: kdeconnect-cli.cpp:104
#, kde-format
msgid "Mount filesystem of a said device"
msgstr "Монтиране на файловата система на посоченото устройство"

#: kdeconnect-cli.cpp:105
#, kde-format
msgid "Print mount point for the filesystem of a said device"
msgstr ""
"Показване на точката за монтиране на файловата система на посоченото "
"устройство"

