msgstr "分享文件/URL到设备"

#: kdeconnect-cli.cpp:88
#, kde-format
msgid "Share text to device"
msgstr "分享文本到设备"

#: kdeconnect-cli.cpp:89
#, kde-format
msgid "Display notifications on device"
msgstr "在设备上显示通知"

#: kdeconnect-cli.cpp:90
#, kde-format
msgid "Lock the specified device"
msgstr "锁定指定设备"

#: kdeconnect-cli.cpp:91
#, kde-format
msgid "Unlock the specified device"
msgstr "解锁指定设备"

#: kdeconnect-cli.cpp:92
#, kde-format
msgid "Sends an SMS. Requires destination"
msgstr "发送短信。需要目标地址"

#: kdeconnect-cli.cpp:93
#, kde-format
msgid "Phone number to send the message"
msgstr "发送信息的手机号码"

#: kdeconnect-cli.cpp:93
#, kde-format
msgid "phone number"
msgstr "手机号码"

#: kdeconnect-cli.cpp:95
#, kde-format
