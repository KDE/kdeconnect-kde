#: kdeconnect-cli.cpp:68
#, kde-format
msgid "List all devices"
msgstr "列出所有设备"

#: kdeconnect-cli.cpp:70
#, kde-format
msgid "List available (paired and reachable) devices"
msgstr "列出可用 (已配对且可访问) 设备"

#: kdeconnect-cli.cpp:71
#, kde-format
msgid "List all backends and status"
msgstr "列出所有后端程序和状态"

#: kdeconnect-cli.cpp:72
#, kde-format
msgid "Enable the specified backend"
msgstr "启用指定的后端程序"

#: kdeconnect-cli.cpp:72 kdeconnect-cli.cpp:73
#, kde-format
msgid "backend"
msgstr "后端程序"

#: kdeconnect-cli.cpp:73
#, kde-format
msgid "Disable the specified backend"
msgstr "禁用指定的后端程序"

#: kdeconnect-cli.cpp:75
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id, to ease "
"scripting"
msgstr "使 --list-devices 或 --list-available 只输出设备 id，方便脚本化"

#: kdeconnect-cli.cpp:77
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices name, to ease "
"scripting"
msgstr "让 --list-devices 或 --list-available 只显示设备名称，方便写脚本"

#: kdeconnect-cli.cpp:79
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id and name, "
"to ease scripting"
msgstr "使 --list-devices 或 --list-available 只输出设备 id 和名称，方便脚本化"

#: kdeconnect-cli.cpp:80
#, kde-format
msgid "Search for devices in the network and re-establish connections"
msgstr "搜索网络中的设备并且重新连接"

#: kdeconnect-cli.cpp:81
#, kde-format
msgid "Request pairing with device"
msgstr "请求与设备配对"

#: kdeconnect-cli.cpp:82
#, kde-format
msgid "Find device by ringing it"
msgstr "通过响铃来查找设备"

#: kdeconnect-cli.cpp:83
#, kde-format
msgid "Unpair from device"
msgstr "取消设备配对"

#: kdeconnect-cli.cpp:84
#, kde-format
msgid "Send ping to device"
msgstr "发送 ping 到设备"

#: kdeconnect-cli.cpp:85
#, kde-format
msgid "Same as ping but you can set the message to display"
msgstr "和 ping 相同但是您可以设置要显示的消息"

#: kdeconnect-cli.cpp:85 kdeconnect-cli.cpp:92
#, kde-format
msgid "message"
msgstr "消息"

#: kdeconnect-cli.cpp:86
#, kde-format
msgid "Send clipboard to device"
msgstr "发送剪贴板到设备"

#: kdeconnect-cli.cpp:87
#, kde-format
