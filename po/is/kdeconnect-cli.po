#, kde-format
msgid "1 device found"
msgid_plural "%1 devices found"
msgstr[0] "1 tæki fannst"
msgstr[1] "%1 tæki fundust"

#: kdeconnect-cli.cpp:166
#, kde-format
msgid "No devices found"
msgstr "Engin tæki fundust"

