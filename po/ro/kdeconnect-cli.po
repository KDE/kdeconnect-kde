msgstr "ID dispozitiv"

#: kdeconnect-cli.cpp:98
#, kde-format
msgid "Device Name"
msgstr "Denumire dispozitiv"

#: kdeconnect-cli.cpp:99
#, kde-format
msgid "Get encryption info about device"
msgstr "Obține informații de criptare despre dispozitiv"

#: kdeconnect-cli.cpp:100
#, kde-format
msgid "List remote commands and their ids"
msgstr "Enumeră comenzi distante și id-urile acestora"

#: kdeconnect-cli.cpp:101
#, kde-format
msgid "Execute remote command by id"
msgstr "Execută comanda distantă după id"

#: kdeconnect-cli.cpp:102
#, kde-format
msgid "Send keys to device"
msgstr "Trimite taste către dispozitiv"

#: kdeconnect-cli.cpp:103
#, kde-format
